{
	"compilerOptions": {
		"target": "ES2022",
		"module": "Node16",
		"moduleResolution": "node16",
		"noEmit": false,
		"rootDir": ".",
		"inlineSourceMap": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"lib": [ "ES2021" ],
		"typeRoots": [ "./node_modules/@types" ],
	},
	"include": [
		"./*.mts"
	],
}
