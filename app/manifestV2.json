{
	"name": "Interceptor",
	"description": "Interceptor",
	"version": "0.0.0",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "html/popup.html",
		"default_icon": {
			"128": "img/head-not-active.png"
		}
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "{3c236fbc-9114-43ed-a224-0cd1834aec4d}",
			"strict_min_version": "110.0"
		}
	},
	"background": {
		"page": "html/background.html"
	},
	"icons": {
		"128": "img/head.png",
		"400": "img/LOGOA_400x400.png"
	},
	"permissions": [
		"activeTab",
		"<all_urls>",
		"storage"
	],
	"web_accessible_resources": ["vendor/*", "js/*", "inpage/*"],
	// all hashes except the first one are from .html files `<script type = 'module'>import '../js/popup.js'</script>` part
	// this was done to workaround bug: https://bugzilla.mozilla.org/show_bug.cgi?id=1803984
	// the first hash is for importmaps: <script type = 'importmap'> ... </script>
	"content_security_policy": "object-src 'self'; script-src 'self' 'sha256-UyyaCZUsudYpMz1pVcm+Zt52+oJyq+lsIZ258a7ufOM=' 'sha256-NKn9HNoZ5CU/EZeR4NUHWTM1SAsoy4LyW7lckzBnq+Y=' 'sha256-Uit0MJakCHko/a4ufXGIQJssrw/9TvLamXN+6ltsoG8=' 'sha256-H3mVX1ZCLoCDyvKnpCmo127wWUkke4ADcFzg0RfP2ZM=' 'sha256-8HfgBeyLUz3lfmitlAsCqVcTegmlIKg2JkhG9r1SC3o=' 'sha256-0TgMBbiL41Lbex4DiXR11hCqokTcIKXw6/HJa+UJ7H4=' 'sha256-X9JOx19AgjNibvT805ows7My7i7Soq9cP0ELHgAiHKg=' 'sha256-gVIjW5dVlcD3TUPzJPpC1f6BIyZdWpfYxyJVWNRAlLg=' 'sha256-jdLyp+TN4F0TZX3YAP3hirJuNLcF15h/mfClKJIQq/k='"
}
